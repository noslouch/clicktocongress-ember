<div class="representative-leftside">
  <h3>{{rep.fullTitle}} {{rep.firstName}} {{rep.lastName}}</h3>
  <p>{{rep.fullParty}}</p>
  {{#if isTouch}}
  <p>Phone: <a href="tel:{{rep.phone}}">{{rep.phone}}</a></p>
  {{else}}
  <p>Phone: {{rep.phone}}</p>
  {{/if}}
</div>

<div class="representative-rightside">
  <button class="representative-callbutton" {{action 'callRep' rep.phone}}>
    Call this person
  </button>
</div>
